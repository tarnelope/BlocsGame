package Tetrominos;

import com.bloc.blocs.TetrisBoard;

public class SquarePiece extends Tetromino {

	public SquarePiece(int x, int y, int numTilesWide, int numTilesTall,
			boolean orientation) {
		super(x, y, numTilesWide, numTilesTall, orientation);
		// TODO Auto-generated constructor stub
	}
	
	@Override
	protected void drawSprite() {
		
	}

	@Override
	protected float getOrigHeight() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	protected float getOrigWidth() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	protected void rotate() {
		s.setRotationCenter(TetrisBoard.TILE_DIMEN, TetrisBoard.TILE_DIMEN);
		s.setRotation(s.getRotation()+90);
	}

}
