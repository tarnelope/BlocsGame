package com.bloc.blocs;

import java.util.ArrayList;

public class Log extends Tetromino {
	
	ArrayList<Tile> tiles;
	
	public Log(float x, float y, boolean orientation) {
		super(Math.round(TetrisBoard.RIGHT_X-5*TetrisBoard.TILE_DIMEN), Math.round(TetrisBoard.TOP_Y-2*TetrisBoard.TILE_DIMEN), 1, 4, false);
	}
	
	private void logSprite() {
		tiles = new ArrayList<Tile>();
		for (int i = 0; i < NUM_OF_TILES; i++) {
			Tile t = new Tile(START_COL, START_ROW+i);
			tiles.add(t);
		}
	}
	
	@Override
	public void rotate90CW() {
		// TODO Auto-generated method stub
		
	}
}
